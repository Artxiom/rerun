    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug 'viewer'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--package=viewer"
                ],
                "filter": {
                    "name": "viewer",
                    "kind": "bin"
                }
            },
            "args": [
                "../recording.rrd"
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}
